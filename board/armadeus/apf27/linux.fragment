  # APF27_Dev_Full have an MAX1027 ADC chip
CONFIG_MAX1027=y

# APF27_Dev_Full have an MAX5821 DAC chip
CONFIG_MAX5821=y

# APF27_Dev_Full have an MCP2515 CAN chip
CONFIG_CAN=y
CONFIG_CAN_MCP251X=y

# APF27_Dev_Full have an AD9889 HDMI transmitter
CONFIG_MEDIA_SUBDRV_AUTOSELECT=n
CONFIG_VIDEO_AD9389B=y

# APF27_Dev_Full have an TSC2101 chip
CONFIG_SPI_TSC2102=y
CONFIG_TOUCHSCREEN_TSC2102=y

